#include <cs50.h>
#include <stdio.h>
int main(void)
{
    
    int number = get_int("Heigh:");

    for( int i = 1; i <= number; i++)
    {
       for(int k = number; k >= 1; k++) 
       {
           printf(".");
       }
        for( int j = 1; j <= i; j++)
        {
            printf("#");    
        }
        printf("\n");
     
    }
}
